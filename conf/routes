# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index
GET     /hackathon                  controllers.Application.hackathon
GET		/stuff						controllers.Services.getReducedEvents(start: Int ?= 1, rows: Int ?=10, search: Option[String])
GET		/stuff/test					controllers.Services.getTestReducedEvents
POST	/trainingSet				controllers.Services.classify

GET		/events						controllers.Services.getReducedEventsFromIds(ids: List[Long])


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
